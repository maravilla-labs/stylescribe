      "$description": "Extra large layered shadow"
    },

    "2xl": {
      "$value": {
        "offsetX": "0px",
        "offsetY": "25px",
        "blur": "50px",
        "spread": "-12px",
        "color": "alpha({color.black}, 0.25)"
      },
      "$type": "shadow",
      "$description": "Dramatic shadow with 25% opacity"
    },

    "brand": {
      "$value": {
        "offsetX": "0px",
        "offsetY": "4px",
        "blur": "14px",
        "spread": "0px",
        "color": "alpha({color.brand}, 0.4)"
      },
      "$type": "shadow",
      "$description": "Brand colored shadow using alpha() with brand color reference"
    },

    "inset": {
      "$value": {
        "offsetX": "0px",
        "offsetY": "2px",
        "blur": "4px",
        "spread": "0px",
        "color": "alpha({color.black}, 0.06)",
        "inset": true
      },
      "$type": "shadow",
      "$description": "Inset shadow for pressed states"
    }
